<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.agilesoftware.bookgonara.bookgo">

    <!-- Permission for internet -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.agilesoftware.bookgonara.bookgo.Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.agilesoftware.bookgonara.bookgo.Activity.HomeBeforeSignInActivity">
        </activity>
        <activity android:name="com.agilesoftware.bookgonara.bookgo.Activity.HomeAfterSignInActivity">
        </activity>
        <activity android:name="com.agilesoftware.bookgonara.bookgo.Activity.SignUpActivity">
        </activity>
        <activity android:name="com.agilesoftware.bookgonara.bookgo.Activity.SettingActivity">
        </activity>
        <activity android:name="com.agilesoftware.bookgonara.bookgo.Activity.ProductDetailActivity">
        </activity>
        <activity android:name="com.agilesoftware.bookgonara.bookgo.Activity.SalesRegistrationActivity">
        </activity>
    </application>

</manifest>